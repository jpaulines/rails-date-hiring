<div class="container">

  <!-- tabs for my events & my bookings -->
  <ul class="nav nav-pills mb-4 mt-4" id="pills-tab" role="tablist">
    <li class="nav-item">
      <a class="nav-link active" id="pills-events-tab" data-toggle="pill" href="#pills-events" role="tab" aria-controls="pills-events" aria-selected="true">Your Events</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" id="pills-bookings-tab" data-toggle="pill" href="#pills-bookings" role="tab" aria-controls="pills-bookings" aria-selected="false">Your Bookings</a>
    </li><li class="nav-item">
      <a class="nav-link" id="pills-ahead-tab" data-toggle="pill" href="#pills-ahead" role="tab" aria-controls="pills-ahead" aria-selected="false">Events ahead</a>
    </li>
  </ul>

  <div class="tab-content" id="pills-tabContent">
    <div class="tab-pane fade show active" id="pills-events" role="tabpanel" aria-labelledby="pills-events-tab">
      <%= render 'my-events'%>
    </div>
    <div class="tab-pane fade" id="pills-bookings" role="tabpanel" aria-labelledby="pills-bookings-tab">
      <%= render 'my-bookings'%>
    </div>
    <div class="tab-pane fade" id="pills-ahead" role="tabpanel" aria-labelledby="pills-ahead-tab">
      <%= render 'events-ahead'%>
    </div>
  </div>
</div>
